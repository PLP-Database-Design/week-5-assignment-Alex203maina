<table>
    <thead>
        <tr>
            <th>Patient ID</th>
            <th>first name</th>
            <th>Last name</th>
            <th> </th>
            <th></th>
        </tr>
    </thead>
        <tbody>
            <% data.forEach(row => {%>
                <tr>
                    <td><%= row.patient_id %> </td>
                    <td><%= row.first_name %> </td>
                    <td><%= row.last_name %> </td>
                </tr>
            <%});%>
        </tbody>
</table>